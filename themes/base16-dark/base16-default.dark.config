[global_config]
  always_split_with_profile = True
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  tab_position = bottom
  title_font = Source Code Pro for Powerline 12
  title_hide_sizetext = True
  title_use_system_font = False
  title_transmit_bg_color = "#ab4642"
  title_inactive_bg_color = "#585858"
  focus = system
[keybindings]
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  go_up = <Alt>k
  layout_launcher = None
  switch_to_tab_1 = <Alt>1
  switch_to_tab_10 = <Alt>
  switch_to_tab_2 = <Alt>2
  switch_to_tab_3 = <Alt>3
  switch_to_tab_4 = <Alt>4
  switch_to_tab_5 = <Alt>5
  switch_to_tab_6 = <Alt>6
  switch_to_tab_7 = <Alt>7
  switch_to_tab_8 = <Alt>8
  switch_to_tab_9 = <Alt>9
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
		font = Source Code Pro for Powerline 12
		icon_bell = False
		scrollback_infinite = True
		scrollbar_position = hidden
		split_to_group = True
		use_system_font = False
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#181818:#ab4642:#a1b56c:#f7ca88:#7cafc2:#ba8baf:#86c1b9:#d8d8d8:#585858:#ab4642:#a1b56c:#f7ca88:#7cafc2:#ba8baf:#86c1b9:#f8f8f8"
    foreground_color = "#d8d8d8"
    background_color = "#181818"
    cursor_color = "#d8d8d8"

  [[Base16 Default Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#181818:#ab4642:#a1b56c:#f7ca88:#7cafc2:#ba8baf:#86c1b9:#d8d8d8:#585858:#ab4642:#a1b56c:#f7ca88:#7cafc2:#ba8baf:#86c1b9:#f8f8f8"
    foreground_color = "#d8d8d8"
    background_color = "#181818"
    cursor_color = "#d8d8d8"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Default Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
